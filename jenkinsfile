
	
	pipeline{
	
	
	agent any
	stages{
	
		stage('compile'){
			steps{
				bat 'mvn compile'
			}
		}
		stage('test'){
			steps{
				bat 'mvn test'
			}
		}
		
		stage('build'){
			steps{
				bat 'mvn install'
				bat 'cd target '
				bat 'java -jar nobroker-1.0-SNAPSHOT.jar '
			}
		}
		
		
		
	}
}
